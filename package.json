{
  "name": "@flowbie/n8n-nodes-transcribe-video",
  "version": "1.0.0",
  "description": "n8n node for Flowbie video transcription API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "flowbie",
    "video",
    "transcription",
    "whisper"
  ],
  "license": "MIT",
  "homepage": "https://flowbie.ai",
  "author": {
    "name": "Flowbie",
    "email": "support@flowbie.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flowbie-ai/n8n-nodes-transcribe-video.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/FlowbieApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Flowbie/Flowbie.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^24.8.1",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
